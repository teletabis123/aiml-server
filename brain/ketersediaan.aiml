<?xml version="1.0" encoding="UTF-8"?>
<aiml>

    <category>
        <pattern>CEK SEDIA KAMAR</pattern>
        <template>
            <think><set name="sedia">kamar</set></think>
            Kamar yang tersedia untuk dipesan:
            [ul]
                [li]Deluxe[/li]
                [li]Club Premiere[/li]
                [li]Deluxe Suite[/li]
                [footer]Tipe kamar apa yang ingin Anda cek ketersediannya?[/footer]
            [/ul]
        </template>
    </category>

    <category>
        <pattern>KAMAR *</pattern>
        <template>
            <condition name = "sedia">
                <srai><star index="1"/></srai>
            </condition>
        </template>
    </category>
    
